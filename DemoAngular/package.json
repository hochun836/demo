    "@types/hammerjs": "^2.0.41",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.1.0",
    "@types/sortablejs": "^1.15.1",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.0.2"
  }
