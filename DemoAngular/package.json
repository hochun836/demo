{
  "name": "demo-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "public": "ng serve --host=0.0.0.0",
    "build-sit": "ng build --configuration=sit --output-path=dist/sit --base-href=/demo-angular/ && copyfiles .htaccess dist/sit && node open-file-explorer dist/sit",
    "build-prd": "ng build --configuration=production --output-path=dist/prd --base-href=/demo-angular/ && copyfiles .htaccess dist/prd && node open-file-explorer dist/prd"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/cdk": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/material": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "animate.css": "^4.1.1",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "hammerjs": "^2.0.8",
    "path": "^0.12.7",
    "rxjs": "~7.8.0",
    "sortablejs": "^1.15.0",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.13.0"
